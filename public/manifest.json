{
  "manifest_version": 3,
  "name": "Easy Paste",
  "version": "1.0.0",
  "description": "Sticky note extension to make developer, tester and normal users life easy.",
  "permissions": [],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon18.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon18.png",
    "48": "icon18.png",
    "128": "icon18.png"
  }
}
